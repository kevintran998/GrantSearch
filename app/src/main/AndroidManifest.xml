<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kkt160130.grantsearch">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.kkt160130.grantsearch.TapToStartActivity"></activity>
        <activity android:name="com.kkt160130.grantsearch.GrantSearchResultsActivity" />
        <activity android:name="com.kkt160130.grantsearch.GrantActivity" />
        <activity android:name="com.kkt160130.grantsearch.MainActivity" />
        <activity android:name="com.kkt160130.grantsearch.WelcomeScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
